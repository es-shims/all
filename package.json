		"set.prototype.symmetricdifference": "^1.0.2",
		"set.prototype.union": "^1.0.2",
		"string.fromcodepoint": "^1.0.0",
		"string.prototype.at": "^1.0.4",
		"string.prototype.iswellformed": "^1.0.0",
		"string.prototype.lastindexof": "^1.0.2",
		"string.prototype.matchall": "^4.0.8",
		"string.prototype.padend": "^3.1.4",
		"string.prototype.padstart": "^3.1.4",
		"string.prototype.replaceall": "^1.0.7",
		"string.prototype.split": "^1.0.7",
		"string.prototype.substr": "^1.0.1",
		"string.prototype.towellformed": "^1.0.0",
		"string.prototype.trim": "^1.2.7",
		"string.prototype.trimend": "^1.0.6",
		"string.prototype.trimleft": "^2.1.3",
		"string.prototype.trimright": "^2.1.3",
		"string.prototype.trimstart": "^1.0.6",
		"string.raw": "^1.0.6",
		"suppressed-error": "^1.0.0",
		"symbol.prototype.description": "^1.0.5"
	},
	"devDependencies": {
		"@browserify/envify": "^6.0.0",
		"@browserify/uglifyify": "^6.0.0",
		"@ljharb/eslint-config": "^21.0.1",
		"aud": "^2.0.2",
		"browserify": "^17.0.0",
		"bundle-collapser": "^1.4.0",
		"common-shakeify": "~1.0.0",
		"eslint": "=8.8.0",
		"in-publish": "^2.0.1",
		"module-deps": "^6.2.3",
		"npmignore": "^0.3.0",
		"safe-publish-latest": "^2.0.0",
		"test262": "github:tc39/test262",
		"test262-harness": "^10.0.0",
		"unassertify": "^3.0.1"
	},
	"publishConfig": {
		"ignore": [
			"!browser.js",
			".github/workflows"
		]
	}
}
