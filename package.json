{
	"name": "@es-shims/all",
	"version": "0.0.0",
	"description": "All of the es-shims, auto-installed into the environment",
	"main": "index.js",
	"browser": {
		".": "./browser/index.js"
	},
	"exports": {
		".": [
			{
				"browser": "./browser.js",
				"default": "./index.js"
			},
			"./index.js"
		],
		"./package.json": "./package.json"
	},
	"scripts": {
		"deps": "module-deps index.js | node deps",
		"postdeps": "cat paths.json",
		"build": "browserify -g unassertify -g @browserify/envify -g [@browserify/uglifyify --mangle.keep_fnames --compress.keep_fnames --format.indent_level=1 --compress.arrows=false --compress.passes=4 --compress.typeofs=false] -p common-shakeify -p bundle-collapser/plugin index.js > browser.js",
		"postbuild": "ls -oh browser.js | awk '{print $4, $8}'",
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublishOnly": "safe-publish-latest && npm run build",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"find-dupes": "npm ls --all --omit=dev --parseable | xargs -L1 sh -c 'basename $1' dummy | sort | uniq -c | grep -v \"^ *1 \" | sort -rn",
		"lint": "eslint --ext=js,mjs .",
		"pretest": "npm run lint",
		"pretests-only": "npm run build",
		"tests-only": "test262-harness -t \"$(node -pe 'os.cpus().length - 1')\" --prelude=./browser.js --host-args='--unhandled-rejections=none' --features-exclude='resizable-arraybuffer,regexp-unicode-property-escapes,regexp-named-groups,regexp-v-flag,cross-realm,TypedArray,Temporal,Atomics,ShadowRealm,Proxy,FinalizationRegistry,WeakRef,symbols-as-weakmap-keys,regexp-duplicate-named-groups' --preprocessor=./test/preprocessor.js --test262-dir=node_modules/test262 'node_modules/test262/test/built-ins/**/*.js'",
		"test": "npm run tests-only",
		"posttest": "aud --production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/all.git"
	},
	"keywords": [
		"es-shim",
		"es-shims",
		"es-shim API",
		"polyfill",
		"shim",
		"prollyfill"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/all/issues"
	},
	"homepage": "https://github.com/es-shims/all#readme",
	"dependencies": {
		"array-includes": "^3.1.6",
		"array.from": "^1.1.3",
		"array.of": "^1.0.1",
		"array.prototype.at": "^1.1.1",
		"array.prototype.concat": "^1.0.3",
		"array.prototype.copywithin": "^1.1.1",
		"array.prototype.entries": "^1.1.1",
		"array.prototype.every": "^1.1.4",
		"array.prototype.filter": "^1.0.2",
		"array.prototype.find": "^2.2.1",
		"array.prototype.findindex": "^2.2.1",
		"array.prototype.findlast": "^1.2.2",
		"array.prototype.findlastindex": "^1.2.2",
		"array.prototype.flat": "^1.3.1",
		"array.prototype.flatmap": "^1.3.1",
		"array.prototype.foreach": "^1.0.4",
		"array.prototype.group": "^1.1.2",
		"array.prototype.grouptomap": "^1.1.2",
		"array.prototype.indexof": "^1.0.5",
		"array.prototype.join": "^1.0.1",
		"array.prototype.keys": "^1.1.1",
		"array.prototype.lastindexof": "^1.0.4",
		"array.prototype.map": "^1.0.5",
		"array.prototype.push": "^1.0.3",
		"array.prototype.reduce": "^1.0.5",
		"array.prototype.reduceright": "^1.0.5",
		"array.prototype.slice": "^1.0.1",
		"array.prototype.some": "^1.1.4",
		"array.prototype.splice": "^1.0.2",
		"array.prototype.toreversed": "^1.1.1",
		"array.prototype.tosorted": "^1.1.1",
		"array.prototype.tospliced": "^1.1.1",
		"array.prototype.unshift": "^1.0.1",
		"array.prototype.values": "^1.1.1",
		"array.prototype.with": "^1.1.2",
		"arraybuffer.prototype.detached": "^1.0.0",
		"arraybuffer.prototype.transfer": "^1.0.2",
		"arraybuffer.prototype.transfertofixedlength": "^1.0.0",
		"date": "^2.0.2",
		"error-cause": "^1.0.5",
		"es-abstract": "^1.21.1",
